{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "common": ["src/lib/common"],
      "common/*": ["src/lib/common/*"],
      "styles": ["src/styles"],
      "styles/*": ["src/styles/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"]
    }
  }
}
