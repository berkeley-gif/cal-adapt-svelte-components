{
  "name": "cal-adapt-svelte-components",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "svelte-kit dev -p 3333 -o",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && husky install",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:styles": "stylelint \"**/*.{css,scss,sass,svelte}\" --cache --ignore-path .gitignore --fix --allow-empty-input",
    "sass:check": "sass --load-path=./node_modules src/styles/main.scss > /dev/null",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.16.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^27.5.1",
    "carbon-components": "^10.56.0",
    "carbon-components-svelte": "^0.42.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.49.9",
    "stylelint": "^14.6.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.6",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --cache",
    "*.{css,scss,svelte}": "stylelint --allow-empty-input",
    "*.{js,svelte,jsx,ts,tsx,md,html,css,scss}": "prettier --write"
  }
}
